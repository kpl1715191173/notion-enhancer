    {
      "type": "toggle",
      "key": "numbered_list",
      "label": "numbered lists",
      "value": true
    },
    {
      "type": "toggle",
      "key": "to_do",
      "label": "to-do lists",
      "value": true
    },
    {
      "type": "toggle",
      "key": "toggle",
      "label": "toggle lists",
      "value": true
    },
    {
      "type": "toggle",
      "key": "table_of_contents",
      "label": "tables of contents",
      "value": true
    },
    {
      "type": "toggle",
      "key": "outliner",
      "label": "outliner (panel extension)",
      "value": true
    }
  ]
}
