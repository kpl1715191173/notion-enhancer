{
  "author": {
    "name": "Sentry"
  },
  "dependencies": {
    "@sentry/hub": "5.27.6",
    "@sentry/types": "5.27.6",
    "tslib": "^1.9.3"
  },
  "deprecated": false,
  "description": "Sentry minimal library that can be used in other packages",
  "devDependencies": {
    "@sentry-internal/eslint-config-sdk": "5.27.6",
    "eslint": "7.6.0",
    "jest": "^24.7.1",
    "npm-run-all": "^4.1.2",
    "prettier": "1.19.0",
    "rimraf": "^2.6.3",
    "typescript": "3.7.5"
  },
  "engines": {
    "node": ">=6"
  },
  "homepage": "https://github.com/getsentry/sentry-javascript/tree/master/packages/minimal",
  "jest": {
    "collectCoverage": true,
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.ts"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "./tsconfig.json",
        "diagnostics": false
      }
    }
  },
  "license": "BSD-3-Clause",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "name": "@sentry/minimal",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/getsentry/sentry-javascript.git"
  },
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "version": "5.27.6",
  "volta": {
    "extends": "../../package.json"
  }
}